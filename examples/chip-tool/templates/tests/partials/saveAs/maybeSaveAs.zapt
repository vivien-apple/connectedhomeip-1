{{#if saveAs}}
    {{#if (isString type)}}
        if ({{saveAs}}Buffer != nullptr)
        {
            chip::Platform::MemoryFree({{saveAs}}Buffer);
        }
        {{saveAs}}Buffer = static_cast<{{#if (isOctetString type)}}uint8_t{{else}}char{{/if}} *>(chip::Platform::MemoryAlloc({{asRootObject}}.size()));
        memcpy({{saveAs}}Buffer, {{asRootObject}}.data(), {{asRootObject}}.size());
        {{saveAs}} = {{chipType}}({{saveAs}}Buffer, {{asRootObject}}.size());
    {{else}}
        {{saveAs}} = {{asRootObject}};
    {{/if}}
{{/if}}
